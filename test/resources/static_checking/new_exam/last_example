P(id:String)
Public
;
Public flows P
;

(csm:consume) (P(id:Bot)) [?:String] [P(#id) -> P(id=*)]
;
csm -->> {
  (rply: reply) (@trigger.initiator, @trigger.receiver) [?] [@Receiver(csm) -> @Initiator(csm)]
  ;
  rply -->> {
    (yea: accept) (@trigger$csm.initiator, @trigger$csm.receiver) [?] [@Initiator(csm) -> @Receiver(csm)]
    (nay: reject)  (@trigger$csm.initiator, @trigger$csm.receiver) [?] [@Initiator(csm) -> @Receiver(csm)]
    ;
    yea, nay -->% yea, nay
  }
}